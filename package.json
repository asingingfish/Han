{
    "name": "han",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-node": "cross-env PORT=9001 nodemon dist/bin/www.js",
        "watch-ts": "tsc -w",
        "build": "tsc",
        "serve": "cross-env PORT=9001 node dist/bin/www.js"
    },
    "bin": {},
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "compression": "^1.7.4",
        "cross-env": "^5.2.0",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "moment": "^2.24.0",
        "mysql": "^2.17.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/express": "^4.16.1",
        "@types/moment": "^2.13.0",
        "@types/mysql": "^2.15.5",
        "@types/shelljs": "^0.8.5",
        "concurrently": "^4.1.0",
        "nodemon": "^1.18.11",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.3"
    }
}
